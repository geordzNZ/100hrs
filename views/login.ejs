    <main class="">
        <div class="container">
            <div class="row">
                <div class="col s12 center-align">
                  <h1 class="indigo-text darken-4">BUILDING<br>MANAGEMENT<br>APP</h1>
                </div>
            </div>
            <div class="row">
              <div class="col s12 center-align">
                <% if (errors) { %> 
                  <div>
                    <i class="material-icons-outlined red lighten-3">error</i>
                    ... FORM VALIDATION ERROR<%= errors.length === 1 ? '' : 'S'%> ...
                    <i class="material-icons-outlined red lighten-3">error</i>
                  </div>
                  <% errors.forEach( err => { %>
                    <div class="alert">
                      <%= err.msg %>
                    </div>
                  <% }) %> <% } %> 
                </div>
            </div>
            <div class="row">
              <form class="col s12 z-depth-4" action="/login" method="POST">
              <!-- <form class="col s12 z-depth-4" action="/dashboardPage" method="GET"> -->
                <div class="row">
                  <div class="input-field col s6 offset-s3">
                    <i class="material-icons prefix">alternate_email</i>
                    <input id="email" name="email" type="text" class="validate">
                    <label for="email">Email Address</label>
                  </div>
                </div>
                <div class="row">
                  <div class="input-field col s6 offset-s3">
                    <i class="material-icons prefix">password</i>
                    <input id="password" name="password" type="password" class="validate">
                    <label for="password">Password</label>
                  </div>
                </div>
                <div class="row">
                  <div class="input-field col s6 offset-s8">
                    <button type="submit" class="btn waves-effect waves-light indigo lighten-1">Login</button>
                  </div>
                  </div>
              </form>
            </div>
            
        </div>
    </main>